mi <- function(x, y) {
joint_table <- table(x, y) / length(x)  # P(X, Y)
px <- table(x) / length(x)              # P(X)
py <- table(y) / length(y)              # P(Y)
mi <- 0
for (i in rownames(joint_table)) {
for (j in colnames(joint_table)) {
pxy <- joint_table[i, j]
if (pxy > 0) {  # Avoid log(0)
mi <- mi + pxy * log(pxy / (px[i] * py[j]))
}
}
}
return(mi)
}
